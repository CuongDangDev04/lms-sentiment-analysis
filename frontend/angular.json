{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",

              "src/assets/lecture_admin/css/bootstrap.min.css",
              "src/assets/lecture_admin/css/plugins.min.css",
              "src/assets/lecture_admin/css/kaiadmin.min.css",
              "src/assets/lecture_admin/css/demo.css",

              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/assets/student/lib/animate/animate.min.css",
              "src/assets/student/lib/owlcarousel/assets/owl.carousel.min.css",
              "src/assets/student/css/bootstrap.min.css",
              "src/assets/student/css/style.css"
            ],
            "scripts": [

              "src/assets/lecture_admin/js/core/jquery-3.7.1.min.js",
              "src/assets/lecture_admin/js/core/popper.min.js",
              "src/assets/lecture_admin/js/core/bootstrap.min.js",
              "src/assets/lecture_admin/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js",
              "src/assets/lecture_admin/js/plugin/chart.js/chart.min.js",
              "src/assets/lecture_admin/js/plugin/bootstrap-notify/bootstrap-notify.min.js",
              "src/assets/lecture_admin/js/plugin/jquery.sparkline/jquery.sparkline.min.js",
              "src/assets/lecture_admin/js/plugin/chart-circle/circles.min.js",
              "src/assets/lecture_admin/js/plugin/datatables/datatables.min.js",
              "src/assets/lecture_admin/js/plugin/jsvectormap/jsvectormap.min.js",
              "src/assets/lecture_admin/js/plugin/jsvectormap/world.js",
              "src/assets/lecture_admin/js/plugin/sweetalert/sweetalert.min.js",
              "src/assets/lecture_admin/js/kaiadmin.min.js",
              "src/assets/lecture_admin/js/setting-demo.js",
              "src/assets/lecture_admin/js/demo.js",

              "src/assets/student/js/main.js",
              "src/assets/student/lib/wow/wow.min.js",
              "src/assets/student/lib/easing/easing.min.js",
              "src/assets/student/lib/waypoints/waypoints.min.js",
              "src/assets/student/lib/owlcarousel/owl.carousel.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "frontend:build:production"
            },
            "development": {
              "buildTarget": "frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "tsconfig.spec.json",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.min.css",
              "src/assets/student/lib/animate/animate.min.css",
              "src/assets/student/lib/owlcarousel/assets/owl.carousel.min.css",
              "src/assets/student/css/bootstrap.min.css",
              "src/assets/student/css/style.css"
            ],
            "scripts": [
              "src/assets/student/js/main.js",
              "src/assets/student/lib/wow/wow.min.js",
              "src/assets/student/lib/easing/easing.min.js",
              "src/assets/student/lib/waypoints/waypoints.min.js",
              "src/assets/student/lib/owlcarousel/owl.carousel.min.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "5980cc28-816a-446c-89a9-45cefc663459"
  }
}